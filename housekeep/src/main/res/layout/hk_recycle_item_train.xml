<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:padding="25dp"
    tools:context="com.ysjkj.smart.community.housekeep.mvp.ui.activity.BabySitterInfoActivity">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="培训名称:保姆岗前培训"
        android:textColor="@color/hk_gray_text_color"
        android:textSize="15dp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="22dp"
        android:text="培训时间:2014/3/4~2017/3/5"
        android:textColor="@color/hk_gray_text_color"
        android:textSize="15dp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="22dp"
        android:text="培训结果:培训合格"
        android:textColor="@color/hk_gray_text_color"
        android:textSize="15dp" />
</LinearLayout>
